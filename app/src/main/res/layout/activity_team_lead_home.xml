<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="@android:color/white"
    android:layout_height="match_parent">

	<RelativeLayout
	    android:id="@+id/banner"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:background="@android:color/darker_gray"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/image2"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:layout_alignParentLeft="true"
            android:layout_marginRight="15dp"
            android:src="@drawable/site_logo" />

        <TextView
            android:id="@+id/clinictext"
            style="@style/FontSizeMedium"
            android:layout_width="500dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="30dp"
            android:layout_marginTop="12dp"
            android:layout_toRightOf="@+id/image2"
            android:text="@string/clinic_name"
            android:textColor="@android:color/black" />

        <TextView
            android:id="@+id/clinictime"
            style="@style/FontSizeMedium"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_toRightOf="@+id/clinictext"
            android:text="Clinic Time:"
            android:textColor="#0079D2" />

        <TextView
            android:id="@+id/time"
            style="@style/FontSizeMedium"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="25dp"
            android:layout_marginTop="12dp"
            android:layout_toRightOf="@+id/clinictime"
            android:clickable="true"
            android:text="@string/time"
            android:textColor="#0079D2" />

        <TextView
            android:id="@+id/home"
            style="@style/FontSizeMedium"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_toRightOf="@+id/time"
            android:text="@string/home"
            android:textColor="#0079D2" />

        <TextView
            android:id="@+id/user"
            style="@style/FontSizeMedium"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="30dp"
            android:layout_marginTop="12dp"
            android:layout_toRightOf="@+id/home"
            android:text="@string/user"
            android:textColor="#0079D2" />

        <TextView
            android:id="@+id/logout"
            style="@style/FontSizeMedium"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_toRightOf="@+id/user"
            android:text="@string/logout"
            android:textColor="#0079D2" />
    </RelativeLayout>
    
    <LinearLayout
        android:id="@+id/team_lead_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        android:layout_below="@+id/banner"        
        android:orientation="vertical">

        <TextView
            android:id="@+id/team_lead_header_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TEAM LEAD HOME"
            android:layout_marginTop="20dp"
            
            style="@style/FontSizeLarge"
            android:textColor="@android:color/black"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/black" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/imagesLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/team_lead_header"
        >

        <ImageView
            android:id="@+id/tl_clinic_schedule"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:src="@drawable/clinic_schedule"
            android:layout_marginRight="100dp"
            />

        <TextView
            android:id="@+id/tl_clinic_schedule_text"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="Clinic Schedule"
            android:gravity="center"
            style="@style/FontSizeLarge"
            android:textColor="@android:color/black"
            android:layout_below="@+id/tl_clinic_schedule"
            android:layout_marginRight="100dp"/>

        <TextView
            android:id="@+id/tl_daily_stats_text"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="Daily Stats"
            style="@style/FontSizeLarge"
            android:textColor="@android:color/black"
            android:layout_marginLeft="250dp"
            
            android:layout_below="@+id/tl_clinic_schedule"
            android:layout_toRightOf="@+id/tl_clinic_info_text"
            />

        <ImageView
            android:id="@+id/tl_staff"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:src="@drawable/clinic_staff"
            android:layout_marginRight="100dp"
            android:layout_below="@+id/tl_clinic_schedule_text"
            android:layout_marginTop="50dp"
            />

        <TextView
            android:id="@+id/team_lead_image4_text"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="Staff"
            android:gravity="center"
            style="@style/FontSizeLarge"
            android:textColor="@android:color/black"
            android:layout_below="@+id/tl_staff"
            android:layout_marginRight="100dp"/>

        <TextView
            android:id="@+id/team_lead_image5_text"
            style="@style/FontSizeLarge"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tl_closeout"
            android:layout_marginLeft="200dp"
            android:layout_toRightOf="@+id/tl_staff"
            
            android:text="Close Out"
            android:textColor="@android:color/black" />

        <TextView
            android:id="@+id/tl_clinic_info_text"
            style="@style/FontSizeLarge"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_above="@+id/tl_staff"
            android:layout_alignLeft="@+id/tl_clinic_info"
            android:layout_marginLeft="21dp"
            android:text="Clinic Info"
            android:textColor="@android:color/black" />

        <ImageView
            android:id="@+id/tl_closeout"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_alignRight="@+id/team_lead_image5_text"
            android:layout_alignTop="@+id/tl_staff"
            android:layout_marginRight="25dp"
            android:src="@drawable/close_out" />

        <ImageView
            android:id="@+id/tl_clinic_info"
            android:layout_width="128dp"
            android:layout_height="128dp"
            android:layout_alignBottom="@+id/tl_clinic_schedule"
            android:layout_alignRight="@+id/tl_closeout"
            android:layout_marginStart="95dp"
            android:src="@drawable/info" />

        <ImageView
            android:id="@+id/tl_daily_stats"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_above="@+id/tl_daily_stats_text"
            android:layout_alignRight="@+id/tl_daily_stats_text"
            android:layout_marginRight="26dp"
            android:layout_marginStart="103dp"
            android:src="@drawable/daily_stats" />

    </RelativeLayout>

</RelativeLayout>